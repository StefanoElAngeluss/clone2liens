<%= render "cart/cart" if @render_cart %>
<hr />
<div class="d-flex">
    <h1>Produits</h1>
</div>

<div id="produits">
<div class="container">
  <% @produits.each do |produit| %>

    <%= render produit %>
    <p>
      <%= link_to "Show this produit", produit %>
    </p>
  <% end %>
</div>
</div>
